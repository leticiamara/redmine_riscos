<div class="contextual">
  <%= link_to_function 'Voltar', "history.back()", :class => 'icon icon-reload' %>
</div>

<h2>Histórico - Risco #<%= @risco.id %> </h2>
<div class="autoscroll" >
<table class="list issues">
  <tr>
    <th>Versão</th>
    <th>Data de Modificação</th>
  </tr>

<% for version in @historicos.reverse %>
  	<tr class="issue">
		<td><%= link_to '#' + version.version.to_s, show_historico_risco_path(:risco => @risco, :versao =>version.version, :project_id => @project.identifier) %></td>
		<td><%=version.updated_at.to_formatted_s(:long) %></td>
	</tr>
<% end %>
</table>
</div>

